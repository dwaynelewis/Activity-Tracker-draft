<p id="notice"><%= notice %></p>

<h1>Young Americanas</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Hours</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @young_americanas.each do |young_americana| %>
      <tr>
        <td><%= young_americana.period %></td>
        <td><%= young_americana.hours %></td>
        <td><%= young_americana.description %></td>
        <td><%= link_to 'Show', young_americana %></td>
        <td><%= link_to 'Edit', edit_young_americana_path(young_americana) %></td>
        <td><%= link_to 'Destroy', young_americana, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Young Americana', new_young_americana_path %>

