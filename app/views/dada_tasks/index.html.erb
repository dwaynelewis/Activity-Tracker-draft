<p id="notice"><%= notice %></p>

<h1>Dada Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Hours</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dada_tasks.each do |dada_task| %>
      <tr>
        <td><%= dada_task.Period %></td>
        <td><%= dada_task.Hours %></td>
        <td><%= dada_task.Description %></td>
        <td><%= link_to 'Show', dada_task %></td>
        <td><%= link_to 'Edit', edit_dada_task_path(dada_task) %></td>
        <td><%= link_to 'Destroy', dada_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dada Task', new_dada_task_path %>

