<p id="notice"><%= notice %></p>

<h1>Sex Eds</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Hours</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sex_eds.each do |sex_ed| %>
      <tr>
        <td><%= sex_ed.Period %></td>
        <td><%= sex_ed.Hours %></td>
        <td><%= sex_ed.Description %></td>
        <td><%= link_to 'Show', sex_ed %></td>
        <td><%= link_to 'Edit', edit_sex_ed_path(sex_ed) %></td>
        <td><%= link_to 'Destroy', sex_ed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

