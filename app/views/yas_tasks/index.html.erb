<p id="notice"><%= notice %></p>

<h1>Yas Tasks</h1>

<table>
  <thead>
    <tr>
      <th>Period</th>
      <th>Hours</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @yas_tasks.each do |yas_task| %>
      <tr>
        <td><%= yas_task.Period %></td>
        <td><%= yas_task.Hours %></td>
        <td><%= yas_task.Description %></td>
        <td><%= link_to 'Show', yas_task %></td>
        <td><%= link_to 'Edit', edit_yas_task_path(yas_task) %></td>
        <td><%= link_to 'Destroy', yas_task, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yas Task', new_yas_task_path %>

